{
  "name": "ai-dpr-system",
  "version": "1.0.0",
  "description": "AI-powered Detailed Project Report system for risk analysis and project management",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "build": "npm run build:frontend",
    "build:frontend": "cd react-frontend && npm run build",
    "install:all": "npm install && cd react-frontend && npm install",
    "setup": "npm run install:all && npm run build",
    "setup:db": "node scripts/setup-database.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "deploy": "npm run setup && npm start",
    "heroku-postbuild": "npm run build:frontend"
  },
  "keywords": [
    "ai",
    "project-management",
    "risk-analysis",
    "reporting",
    "supabase",
    "nodejs",
    "react"
  ],
  "author": "AI DPR Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/ai-dpr-system.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/ai-dpr-system/issues"
  },
  "homepage": "https://github.com/your-username/ai-dpr-system#readme"
}
